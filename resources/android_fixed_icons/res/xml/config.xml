        <param name="android-package" value="org.apache.cordova.Zip" />
    </feature>
    <feature name="Badge">
        <param name="android-package" value="de.appplant.cordova.plugin.badge.Badge" />
    </feature>
    <feature name="CDVIonicKeyboard">
        <param name="android-package" onload="true" value="io.ionic.keyboard.CDVIonicKeyboard" />
    </feature>
    <allow-navigation href="http://localhost/*" />
    <allow-navigation href="https://localhost/*" />
    <allow-navigation href="ionic://*" />
    <feature name="IonicWebView">
        <param name="android-package" value="com.ionicframework.cordova.webview.IonicWebView" />
    </feature>
    <name>Rando Ardèche</name>
    <description>Geotrek Rando</description>
    <author email="administrateur@parc-monts-ardeche.fr" href="http://www.destination-parc-monts-ardeche.fr/">
        Parc naturel régional des Monts d'Ardèche
    </author>
    <content src="index.html" />
    <access origin="*" />
    <allow-navigation href="http://localhost:8080/*" />
    <allow-navigation href="http://www.destination-parc-monts-ardeche.fr/*" />
    <allow-navigation href="http://gpp3-wxs.ign.fr/*" />
